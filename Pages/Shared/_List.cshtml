@model ShowListViewModel

@if (Model.Listas.Count > 0)
{
    <ul class="list">
        @foreach (var item in Model.Listas)
        {

                <li class="list-item selected">
                    <partial name="_ListItem" model="item" />
                </li>

        }
    </ul>
}
else
{
    <p class="not-found text-center">Não há nenhuma lista</p>
}


